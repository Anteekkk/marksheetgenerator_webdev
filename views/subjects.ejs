<%- include("header") -%>


<h1>SUBJECTS</h1>
<form  class="it" method="post" action="/subjects">
    <div class="form-input">
        <label for="log-in">Subject Code</label>
<input type="number" name="scode" class="form-control" required autofocus id="log-in">
</div>
<div class="form-input">
    <label for="log-in">Subjecct Name</label>
<input type="text" name="sname" class="form-control" required autofocus id="log-in">
</div>
<div class="form-input">
    <label for="log-in">Maximum Marks</label>
<input type="number" name="mm" class="form-control" required autofocus id="log-in">
</div>
<div class="form-input">
    <label for="log-in">Passing Marks</label>
<input type="number" name="pm" class="form-control" required autofocus id="log-in">
</div>
<div class="form-input">
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
    </div>
</form>






